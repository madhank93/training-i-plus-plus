{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/users/clients": {
            "post": {
                "description": "Create a Client in the Database using the data sent by them (REGISTER)",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "/users/clients"
                ],
                "summary": "Create a Client",
                "parameters": [
                    {
                        "description": "User DTO",
                        "name": "clientDTO",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Client"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    }
                }
            }
        },
        "/users/clients/{email}": {
            "get": {
                "description": "Get a Client from the Database using their email.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "/users/clients"
                ],
                "summary": "Get a Client from Database.",
                "parameters": [
                    {
                        "type": "string",
                        "description": "User Email",
                        "name": "email",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.Client"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    }
                }
            },
            "put": {
                "description": "Updates the Client in the Database using their email.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "/users/clients"
                ],
                "summary": "Updates Clients in the Database.",
                "parameters": [
                    {
                        "description": "User DTO",
                        "name": "clientDTO",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Client"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    }
                }
            },
            "delete": {
                "description": "Deletes the Clients in the Database using their email.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "/users/clients"
                ],
                "summary": "Deletes Clients in the Database.",
                "parameters": [
                    {
                        "type": "string",
                        "description": "User Email",
                        "name": "email",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    }
                }
            }
        },
        "/users/experts/diagnose": {
            "post": {
                "description": "Sends a Disease Diagnosis using Kafka to the bookkeeping_service",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "/users/experts/diagnose"
                ],
                "summary": "Make Disease Diagnosis by expert to the bookkeeping_service.",
                "parameters": [
                    {
                        "description": "The Diagnosed Disease",
                        "name": "disease",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Disease"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.Client"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dtos.HTTPError"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dtos.HTTPError": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 400
                },
                "details": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "invalid email"
                    ]
                },
                "message": {
                    "type": "string",
                    "example": "status bad request"
                }
            }
        },
        "models.Client": {
            "type": "object",
            "properties": {
                "email": {
                    "description": "Email of this client",
                    "type": "string"
                },
                "name": {
                    "description": "Name of this client",
                    "type": "string"
                },
                "password": {
                    "description": "Password of this client",
                    "type": "string"
                },
                "subscriptions": {
                    "description": "Subscriptions that this client has",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.ClientSubscription"
                    }
                }
            }
        },
        "models.ClientSubscription": {
            "type": "object",
            "properties": {
                "next_dose": {
                    "type": "string"
                },
                "rate": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "models.Disease": {
            "type": "object",
            "properties": {
                "medicines": {
                    "description": "Medicines that can cure this disease",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "name": {
                    "description": "Name of this disease",
                    "type": "string"
                },
                "symptoms": {
                    "description": "Symptoms of this disease",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    }
}