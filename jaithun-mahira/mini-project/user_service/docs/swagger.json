{
    "swagger": "2.0",
    "info": {
        "description": "Contains User APIs for Train Reservation System",
        "title": "Swagger User Service API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Jaithun Mahira",
            "email": "swiggyb1035@datascience.manipal.edu"
        },
        "license": {
